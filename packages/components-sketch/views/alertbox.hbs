<script src="/symbol_names.js"></script>
<script>
  Array.from(document.querySelectorAll('scale-alertbox')).forEach((btn, i) => {
    btn.dataset.sketchSymbol = `Alertbox / ${order(
      1,
      getCategoryName(btn)
    )} / ${order(
      2,
      btn.dataset.hasIcon ? 'With Icon' : 'Without Icon'
    )} / ${order(4, getStateName(btn))}`;
  });
</script>